        <!-- Fruits Shop Start-->
        <div class="container-fluid fruite py-5">
            <div class="container py-5">
                <div class="tab-class text-center">
                    <div class="row g-4">
                        <div class="col-lg-4 text-start">
                            <h1>Our Organic Products</h1>
                        </div>
                        <div class="col-lg-8 text-end">
                            <ul class="nav nav-pills d-inline-flex text-center mb-5">
                                <% Categories.forEach(item => { %>
                                    <li class="nav-item">
                                        <a class="d-flex py-2 m-2 bg-light rounded-pill" data-bs-toggle="pill" href="#<%= item.category.replace(/\s+/g, '') %>">
                                            <span class="text-dark" style="width: 130px;"><%= item.category %></span>
                                        </a>
                                    </li>
                                <% }) %>
                            </ul>
                        </div>
                    </div>
                    <div class="tab-content">
                        <% Categories.forEach((category, index) => { %>
                            <div id="<%= category.category.replace(/\s+/g, '') %>" class="tab-pane fade show p-0<%= index === 0 ? ' active' : '' %>">
                                <div class="row g-4">
                                    <div class="col-lg-12">
                                        <div class="row g-4">
                                            <% let productCounter = 0; %>
                                            <% Products.forEach(product => { %>
                                                <% if (category.category === product.category && productCounter < limit){ %>
                                                    <div class="col-md-6 col-lg-4 col-xl-3">
                                                        <a href="/shop-details?id=<%= product._id %>" class="text-decoration-none">
                                                            <div class="rounded position-relative fruite-item border border-secondary rounded-bottom">
                                                                <div class="fruite-img" style="height: 240px;">
                                                                    <img src="<%= product.imageUrl %>" class="img-fluid w-100 rounded-top" alt="">
                                                                </div>
                                                                <div class="text-white bg-secondary px-3 py-1 rounded position-absolute" style="top: 10px; left: 10px;"><%= product.category %></div>
                                                                <div class="p-4">
                                                                    <h4><%= product.name %></h4>
                                                                    <p>Lorem ipsum dolor sit amet consectetur adipisicing elit sed do eiusmod te incididunt</p>
                                                                    <div class="d-flex justify-content-between flex-lg-wrap">
                                                                        <p class="text-dark fs-5 fw-bold mb-0"><%= product.sellingPrice %> / kg</p>
                                                                        <form action="/add-to-cart?id=<%= product._id %>" method="post">
                                                                            <button type="submit" class="btn border border-secondary rounded-pill px-3 text-primary">
                                                                                <i class="fa fa-shopping-bag me-2 text-primary"></i> Add to cart
                                                                            </button>
                                                                        </form>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </a>
                                                    </div>
                                                    <% productCounter++; %>
                                                <% } %>
                                            <% }) %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        <% }) %>
                    </div>
                </div>      
            </div>
        </div>
        <!-- Fruits Shop End-->


